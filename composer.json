{
  "name": "damax/api-auth-bundle",
  "type": "symfony-bundle",
  "description": "Authentication via api key or JWT.",
  "license": "MIT",
  "autoload": {
    "psr-4": { "Damax\\Bundle\\ApiAuthBundle\\": "" }
  },
  "require": {
    "php": "^7.1",
    "lcobucci/jwt": "4.*@dev",
    "lcobucci/clock": "1.1.*",
    "symfony/security-bundle": "^3.4|^4.0"
  },
  "require-dev": {
    "doctrine/dbal": "^2.7",
    "friendsofphp/php-cs-fixer": "^2.10",
    "matthiasnoback/symfony-dependency-injection-test": "^3.0",
    "php-coveralls/php-coveralls": "^2.0",
    "predis/predis": "^1.1",
    "symfony/framework-bundle": "^4.1",
    "symfony/phpunit-bridge": "^4.0"
  },
  "scripts": {
    "cs": "php-cs-fixer fix",
    "test": "simple-phpunit",
    "test-cc": "./bin/phpunit-coverage",
    "test-ci": "simple-phpunit --coverage-clover=build/logs/clover.xml",
    "test-publish": "php-coveralls"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.4.x-dev"
    }
  }
}
